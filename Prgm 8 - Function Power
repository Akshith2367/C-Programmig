#include <stdio.h>
int find_pow(int base,int n);
int main() {
    int base,n,a=1,i;
    printf("enter base&n");
    scanf("%d%d",&base,&n);
    a=find_pow(base,n);
    printf("Answer is %d",a);
    return 0;}

int find_pow(int base,int n){
    int i,a=1;
    for(i=1;i<=n;i++){
        a=a*base;}
    return a;}

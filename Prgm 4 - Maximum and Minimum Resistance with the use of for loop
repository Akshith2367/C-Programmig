#include <stdio.h>
int main() {
    int i,n;
    float r,max,min;
    printf("\nEnter The Number of Resistors : ");
    scanf("%d",&n);
    
    for(i=0;i<n;i++){
        printf("\nEnter Resistance of Resistor %d : ",i+1);
        scanf("%f",&r);
        max=max+r;
        min=min+1/r;}
    
    printf("\nMaximum Resistance of Resistors : %f",max);
    printf("\nMinimum Resistance of Resistors : %f",1/min);
    return 0;}
